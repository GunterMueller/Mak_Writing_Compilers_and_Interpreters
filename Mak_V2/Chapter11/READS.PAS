PROGRAM reads (input, output);

VAR
    i : integer;
    x : real;
    c : char;

BEGIN
    write('Type an integer: ');
    readln(i);
    writeln('You typed: ', i);
    writeln;

    write('Type a real: ');
    readln(x);
    writeln('You typed: ', x);
    writeln;

    write('Type a character: ');
    readln(c);
    writeln('You typed: ', c);
    writeln;
END.
